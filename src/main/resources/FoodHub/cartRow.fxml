<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="100.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FoodHub.Control.CartRow">
    <children>
        <Label fx:id="nameLBL" prefWidth="210.0" styleClass="TableLable" text="Kebab" />
        <Label fx:id="costLBL" prefWidth="80.0" styleClass="TableLable" text="100" />
        <Label fx:id="discountLBL" prefWidth="120.0" styleClass="TableLable" text="10" />
        <Label fx:id="countLBL" prefWidth="100.0" styleClass="TableLable" text="2" />
        <Label fx:id="totalLBL" prefWidth="100.0" styleClass="TableLable" text="180" />
        <Button fx:id="removeBTN" mnemonicParsing="false" onAction="#remove" styleClass="TableButton" text="Remove" />
    </children>
    <padding>
        <Insets left="10.0" top="28.0" />
    </padding>
</HBox>
